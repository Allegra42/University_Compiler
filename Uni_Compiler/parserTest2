/** test2 **/

int a;
int b;
int c;
int d;
int r;
int undefined;

a:=4;
b:=7;
c:=-5;
d:=10;


/** Operatorentest **/

r:=a+b;
write(r);

r:=d/c;
write(r);

r:=c-a;
write(r);

r:=c-b;
write(r);

if(a>b){
	write(1);
}else{
	write(0);
};

if(a<:>b){
	write(1);
}else{
	write(0);
};

if(a=b){
	write(1);
}else{
	write(0);
};

/** Funktion des &-Operators ist noch unklar **/
if(a&b){
	write(1);
}else{
	write(0);
};


/** man rechne zur Abwechslung mal mit nichts **/
r:=undefined+1;
write(r);
